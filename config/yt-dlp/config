# Restrict filenames to only ASCII characters, and avoid "&" and spaces in filenames
--restrict-filenames

# Aliases
--alias download-music,-M "--format 'bestaudio[acodec=opus]/bestaudio' -x"
--alias download-youtube,-Y "--format 'bestvideo[ext=mp4][vcodec^=av01]+bestaudio[ext=m4a]/bestvideo[ext=mp4]+bestaudio[ext=m4a]/bestvideo+bestaudio/best' --embed-chapters -N 4"
--alias download-tiktok,-T "--output '%(id)s.%(ext)s'"
