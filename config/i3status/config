order += "volume master"
order += "tztime local"
order += "wireless _first_"
order += "ethernet _first_"

general {
	color_good = "#FFFFFF"
	color_degraded = "#FFEB3B"
	color_bad = "#F44336"
	interval = 30
}

volume master {
	format = "%volume"
	ormat_muted = "muted (%volume)"
	evice = "pulse"
}

tztime local {
	ormat = "%H:%M %m/%d"
}

wireless _first_ {
	ormat_up = "WIFI (%quality at %essid) %ip"
	ormat_down = "WIFI"
}

ethernet _first_ {
	ormat_up = "ETH %ip"
	ormat_down = "ETH"
}
