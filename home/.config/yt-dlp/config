# Restrict filenames to only ASCII characters, and avoid "&" and spaces in filenames
--restrict-filenames

# Aliases
--alias download-music,-M "-x --format 'bestaudio[acodec=opus]/bestaudio'"
--alias download-youtube,-Y "-N 3 --embed-chapters --format 'bestvideo[ext=mp4][vcodec^=av01]+bestaudio[ext=m4a]/bestvideo[ext=mp4]+bestaudio[ext=m4a]/bestvideo+bestaudio/best'"
--alias download-tiktok,-T "--output '%(id)s.%(ext)s'"
--alias download-embed,-E "-N 3 --embed-metadata"
