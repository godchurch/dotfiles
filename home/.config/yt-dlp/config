--restrict-filenames

--alias download-stream,-D "--config-locations '~/.config/yt-dlp/stream.conf'"
--alias download-youtube,-Y "--config-locations '~/.config/yt-dlp/youtube.conf' -o '%(title)s.%(ext)s'"
--alias download-playlist,-L "--config-locations '~/.config/yt-dlp/youtube.conf' -o '%(playlist_title)s/%(playlist_index)s-%(title)s.%(ext)s'"
--alias download-tiktok,-T "--config-locations '~/.config/yt-dlp/tiktok.conf'"

--alias -X "-f 'ba[acodec^=opus]/ba/b' -x"
--alias -l "-S 'res:1080'"
